# Load the package to read .csv files
library(readr)

# Load listings data in R and summarize the variables
listings_ny <- read_csv("data/listings_ny.csv")
View(listings_ny)
summary(listings_ny)

# Load reviews data in R and summarize the variables
reviews_ny <- read_csv("data/reviews_ny.csv")
View(reviews_ny)
summary(reviews_ny)

